<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
    </startup>
    <runtime>
      <legacyCorruptedStateExceptionsPolicy enabled="true"/>
    </runtime>

  <system.diagnostics>
    <trace autoflush="true" indentsize="4">
      <listeners>
        <add name="traceOutput" type="System.Diagnostics.TextWriterTraceListener" initializeData="log.log" />
        <remove name="Default" />
      </listeners>
    </trace>
  </system.diagnostics>
</configuration>